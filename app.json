{
  "expo": {
    "scheme": "parrygg",
    "name": "parrygg",
    "slug": "parrygg",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/imgs/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/imgs/splash.png",
      "resizeMode": "cover",
      "backgroundColor": "#1B1B1B"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "softwareKeyboardLayoutMode": "pan",
      "adaptiveIcon": {
        "foregroundImage": "./assets/imgs/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.parrygg.app"
    },
    "web": {
      "favicon": "./assets/imgs/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "514ca26a-97d4-486e-bf5e-7578123536e7"
      }
    },
    "owner": "blorppppp",
    "plugins": [
      // "expo-router"
    ]
  }
}
